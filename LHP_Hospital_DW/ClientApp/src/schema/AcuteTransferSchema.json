                }
              }
            }
          ]
        }
      },
      "title": "",
      "type": "object",
      "properties": {
        "opportunity": {
          "title": "Opportunity",
          "type": "string",
          "enum": [
            "NP/Telehealth Bypass",
            "Inappropriate/ineffective Treatment Plan",
            "Delay in Initiating Treatment",
            "Lack of/ineffective ACP conversation",
            "Facility Staff not Identifying/Reporting Change in Condition",
            "Delay in care from ancillary services (lab, radiology, pharmacy)",
            "Poor Care Coordination/Communication with PCP and/or specialists",
            "Poor Care Coordination/Communication with Family",
            "Poor Care Coordination/Communication with Facility",
            "Lack of appropriate follow up",
            "Other"
          ]
        }
      }
    }
  },
  "title": "Acute Hospital Transfer",
  "type": "object",
  "properties": {
    "member_banner": {
      "title": "Member Information",
      "type": "string"
    },
    "section_a": {
      "title": "Section A: Hospital Transfer Notification (To Be Completed at Initial Notice of Transfer)",
      "properties": {
        "np_assignment": {
          "title": "Assigned NP",
          "type": "string"
        },
        "pcp_assignment": {
          "title": "Assigned PCP",
          "type": "string"
        },
        "risk_level": {
          "type": "string",
          "title": "Risk Level Prior to Transfer",
          "default": "Pending",
          "enum": [
            "Pending",
            "Low",
            "Medium",
            "High"
          ]
        },
        "baseline_functional_status": {
          "type": "string",
          "title": "Baseline Functional Status",
          "enum": [
            "Independent",
            "One Person Assist",
            "Two Person Assist",
            "Total Dependence"
          ]
        },
        "baseline_congitive_status": {
          "type": "string",
          "title": "Baseline Cognitive Status",
          "enum": [
            "Alert and Oriented x 0",
            "Alert and Oriented x 1",
            "Alert and Oriented x 2",
            "Alert and Oriented x 3",
            "Alert and Oriented x 4"
          ]
        },
        "last_np_visit_date": {
          "title": "Date of Last NP Visit",
          "format": "date",
          "type": "string"
        },
        "transfer_date": {
          "type": "string",
          "format": "date-time",
          "title": "Acute Hospital Transfer Date / Time"
        },
        "transfer_hospital": {
          "readOnly": true,
          "title": "What hospital was the member transferred to?",
          "type": "string"
        },
        "transfer_reason": {
          "title": "Transfer Reason",
          "type": "string"
        },
        "psych_primary": {
          "title": "Psych Primary?",
          "default": "Pending",
          "enum": [
            "Pending",
            "Yes",
            "No"
          ]
        },
        "np_bypass": {
          "title": "NP Bypass",
          "default": "Pending",
          "enum": [
            "Pending",
            "Yes",
            "No"
          ]
        },
        "transfer_authorization": {
          "title": "",
          "dependencies": {
            "who_authorized": {
              "oneOf": [
                {
                  "properties": {
                    "who_authorized": {
                      "enum": [
                        "PCP",
                        "PCP NP",
                        "LHP NP",
                        "PCP / NP Joint Decision",
                        "SNF",
                        "Family",
                        "On Call MD",
                        "Telemedicine",
                        "On Call NP"
                      ]
                    }
                  }
                },
                {
                  "properties": {
                    "who_authorized": {
                      "enum": [
                        "Other"
                      ]
                    },
                    "who_authorized_other": {
                      "title": "Who Authorized Other",
                      "type": "string"
                    }
                  }
                }
              ]
            }
          },
          "properties": {
            "who_authorized": {
              "enum": [
                "PCP",
                "PCP NP",
                "PCP / NP Joint Decision",
                "LHP NP",
                "SNF",
                "Family",
                "On Call MD",
                "Telemedicine",
                "On Call NP",
                "Other"
              ],
              "title": "Who authorized the transfer?",
              "type": "string"
            }
          },
          "type": "object"
        },
        "member_transfer_outcome": {
          "default": "Pending",
          "enum": [
            "Pending",
            "ER Only",
            "Patient Admitted",
            "Patient Under Observation",
            "Patient Expired Prior to Admission",
            "Transfer to other hospital"
          ],
          "title": "Transfer Outcome",
          "type": "string"
        },
        "discharge_date": {
          "type": "string",
          "format": "date",
          "title": "Hospital Discharge Date"
        },
        "business_hours": {
          "type": "string",
          "title": "Was the transfer during business hours?",
          "description": "Mon-Friday 8am-5pm business days (non holidays)",
          "enum": [
            "Yes",
            "No"
          ]
        }
      }
    },

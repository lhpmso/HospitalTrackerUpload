{
  "className": "frmAcuteTransfer",
  "definitions": {
    "discharge": {
      "dependencies": {
        "needs": {
          "oneOf": [
            {
              "properties": {
                "needs": {
                  "type": "array",
                  "contains": {
                    "enum": [
                      "Other"
                    ]
                  }
                },
                "needs_other": {
                  "type": "string",
                  "title": "Other Identified Discharge Need"
                }
              }
            },
            {
              "properties": {
                "needs": {
                  "type": "array",
                  "not": {
                    "contains": {
                      "enum": [
                        "Other"
                      ]
                    }
                  }
                }
              }
            }
          ]
        }
      },
      "type": "object",
      "title": "",
      "properties": {
        "needs": {
          "uniqueItems": true,
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "IV antibiotics/fluids",
              "G tube/PEG tube management",
              "Wound care services",
              "Advanced respiratory services (CPAP, high flow O2, Trach care, Vent care)",
              "Medication review/adjustment",
              "Revisit ACP conversations",
              "Therapy services (OT, PT, ST, RT)",
              "Specialist consult",
              "DME",
              "Alternate facility placement",
              "Other"
            ]
          },
          "title": "Identified Discharge Needs",
          "format": "textarea"
        }
      }
    },
    "actions": {
      "dependencies": {
        "action": {
          "oneOf": [
            {
              "properties": {
                "action": {
                  "type": "array",
                  "contains": {
                    "enum": [
                      "Other"
                    ]
                  }
                },
                "action_other": {
                  "type": "string",
                  "title": "Other Action Taken"
                }
              }
            },
            {
              "properties": {
                "action": {
                  "type": "array",
                  "not": {
                    "contains": {
                      "enum": [
                        "Other"
                      ]
                    }
                  }
                }
              }
            }
          ]
        }
      },
      "type": "object",
      "title": "",
      "properties": {
        "action": {
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "NP education",
              "Outreach to PCP or specialist",
              "Outreach to family",
              "ACP review",
              "Investigation/discussion with ancillary service",
              "Potential Quality Issue",
              "Facility Level Education",
              "Corporate Leadership Follow up",
              "Telehealth Follow Up",
              "Other"
            ]
          },
          "title": "Action taken to mitigate",
          "type": "array"
        }
      }
    },
    "completed_actions": {
      "dependencies": {
        "action": {
          "oneOf": [
            {
              "properties": {
                "action": {
                  "type": "array",
                  "contains": {
                    "enum": [
                      "Other"
                    ]
                  }
                },
                "action_other": {
                  "type": "string",
                  "title": "Other Action Taken"
                }
              }
            },
            {
              "properties": {
                "action": {
                  "type": "array",
                  "not": {
                    "contains": {
                      "enum": [
                        "Other"
                      ]
                    }
                  }
                }
              }
            }
          ]
        }
      },
      "type": "object",
      "title": "",
      "properties": {
        "action": {
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "Education/in-services with facility staff completed",
              "NP education completed",
              "Outreach to PCP or specialist completed",
              "Outreach to family completed",
              "ACP reviewed/scheduled for review",
              "Investigation/discussion with ancillary service completed",
              "Other"
            ]
          },
          "title": "Action taken to mitigate",
          "type": "array"
